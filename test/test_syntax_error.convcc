def calculate_sum(int arr, int n) {
    int sum;
    sum = 0;
    int i;
    i = 0;
    
    for (i = 0; i < n; i = i + 1) {
        sum = sum + arr[i];
    }
    
    return sum;
}

def calculate_average(int arr, int n) {
    int sum;
    sum = calculate_sum(arr, n);
    return sum / n;
}

def find_maximum(int arr, int n) {
    int max_val;
    max_val = arr[0];
    int i;
    i = 1;
    
    for (i = 1; i < n; i = i + 1) {
        if (arr[i] > max_val) {
            max_val = arr[i];
        }
    }
    
    return max_val;
}

