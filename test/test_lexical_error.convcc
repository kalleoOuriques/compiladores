def compute_factorial(int num) {
    if (num <= 1) {
        return 1;
    } else {
        int previous_factorial;
        previous_factorial = compute_factorial(num - 1);
        return num * previous_factorial;
    }
}

def compute_fibonacci(int num) {
    if (num <= 1) {
        return num;
    } else {
        int fib_a;
        int fib_b;
        fib_a = compute_fibonacci(num - 1);
        fib_b = compute_fibonacci(num - 2);
        return fib_a + fib_b;
    }
}

def array_sum_function(int array_param, int size_param) {
    int sum_result;
    sum_result = 0;
    int loop_var;
    loop_var = 0;
    
    for (loop_var = 0; loop_var < size_param; loop_var = loop_var + 1) {
        sum_result = sum_result + array_param[loop_var];
    }
    
    return sum_result;
}

