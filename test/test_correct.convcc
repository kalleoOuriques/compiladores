def is_even(int n) {
    int remainder;
    remainder = n % 2;
    if (remainder == 0) {
        return 1;
    } else {
        return 0;
    }
}

def power(int base, int exp) {
    if (exp == 0) {
        return 1;
    } else {
        return base * power(base, exp - 1);
    }
}

