def fibonacci(int n) {
    if (n <= 1) {
        return n;
    } else {
        int a;
        int b;
        a = fibonacci(n - 1);
        b = fibonacci(n - 2);
        return a + b;
    }
}

def factorial(int n) {
    if (n <= 1) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
}

def sum_array(int arr, int size) {
    int total;
    total = 0;
    int j;
    j = 0;
    
    for (j = 0; j < size; j = j + 1) {
        total = total + arr[j];
    }
    
    return total;
}

